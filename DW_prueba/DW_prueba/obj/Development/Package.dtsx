              <properties>
                <property
                  dataType="System.String"
                  description="Especifica la instrucción SQL utilizada por el componente para extraer datos."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT c.nombre AS Cargo,
e.idEmpleado, e.pnombre + ' ' + e.snombre+' '+papellido AS nombre_completo,d.nombre AS Departamento,
 1 AS version,
ROW_NUMBER() OVER (PARTITION BY e.idEmpleado ORDER BY p.fechaEfectiva DESC) AS version_actual,
C.SueldoBase,
p.fechainicio
FROM Empleado e 
inner JOIN Cargo_empleado CE ON e.idEmpleado = CE.idEmpleado
INNER JOIN Cargo C ON CE.idCargo = C.idCargo
INNER JOIN Empleado_has_Planilla ep ON e.idEmpleado = ep.Empleado_idEmpleado
INNER JOIN Departamento d ON d.iddepartamento = e.iddepartamento
JOIN Planilla p ON ep.Planilla_idPlanilla = p.idPlanilla;</property>
