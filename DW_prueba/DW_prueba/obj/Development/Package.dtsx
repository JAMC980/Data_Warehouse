              <properties>
                <property
                  dataType="System.String"
                  description="Especifica la instrucción SQL utilizada por el componente para extraer datos."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT f.total,c.idcliente,p.idproducto,TO_NUMBER(TO_CHAR(f.fecha, 'YYYYMMDD')) AS idTiempo,s.idservicio,fp.idformapago,h.idhotel,ha.idhabitacion,fd.cantidad
 FROM factura f
INNER JOIN cliente c ON c.idcliente = f.cliente_idcliente
INNER JOIN facturadetalle fd ON fd.factura_idfactura = f.idfactura
INNER JOIN producto p ON p.idproducto = fd.producto_idproducto
INNER JOIN inventario i ON p.inventario_idinventario = i.idinventario
INNER JOIN servicio s ON fd.servicio_idservicio = s.idservicio
INNER JOIN formapago fp ON fp.factura_idfactura = f.idfactura
INNER JOIN hotel h ON s.hotel_idhotel = h.idhotel
INNER JOIN piso pi ON pi.hotel_idhotel = h.idhotel
